{
  "name": "jslike",
  "version": "1.4.2",
  "description": "Production-ready JavaScript interpreter with full ES6+ support using Acorn parser",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./validator": {
      "import": {
        "types": "./dist/validator/index.d.ts",
        "default": "./dist/validator/index.js"
      },
      "require": {
        "types": "./dist/validator/index.d.cts",
        "default": "./dist/validator/index.cjs"
      }
    },
    "./editor/monaco": {
      "import": {
        "types": "./dist/editor/monaco/index.d.ts",
        "default": "./dist/editor/monaco/index.js"
      },
      "require": {
        "types": "./dist/editor/monaco/index.d.cts",
        "default": "./dist/editor/monaco/index.cjs"
      }
    },
    "./editor/wang-prism": {
      "import": {
        "types": "./dist/editor/wang-prism.d.ts",
        "default": "./dist/editor/wang-prism.js"
      },
      "require": {
        "types": "./dist/editor/wang-prism.d.cts",
        "default": "./dist/editor/wang-prism.cjs"
      }
    },
    "./src/editor/wang-prism": {
      "import": {
        "types": "./dist/editor/wang-prism.d.ts",
        "default": "./dist/editor/wang-prism.js"
      },
      "require": {
        "types": "./dist/editor/wang-prism.d.cts",
        "default": "./dist/editor/wang-prism.cjs"
      }
    }
  },
  "bin": {
    "jslike": "./bin/jslike.js"
  },
  "scripts": {
    "build": "node scripts/bundle-acorn.js && tsup && node scripts/post-build.js",
    "build:playground": "node scripts/build-playground.js",
    "start": "node bin/jslike.js",
    "repl": "node bin/repl.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:basic": "node tests/basic.test.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "javascript",
    "interpreter",
    "parser",
    "acorn",
    "es6"
  ],
  "author": "Parth Mudgal <artpar@gmail.com>",
  "license": "MIT",
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "acorn": "^8.15.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.10"
  }
}
